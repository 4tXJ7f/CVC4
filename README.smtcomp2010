#!/bin/bash -ex
#
# This is the version submitted to SMT-COMP 2010.
#
# It was checked out, clean, then compiled with this script.
#
./autogen.sh
./configure competition
make
strip bin/cvc4
make check
mkdir cvc4-smtcomp2010
cp smtcomp2010-runscript cvc4-smtcomp2010/run
cp bin/cvc4 cvc4-smtcomp2010/cvc4
tar cfzv cvc4-smtcomp2010
